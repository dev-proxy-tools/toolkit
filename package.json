{
  "name": "dev-proxy-vsc-ext",
  "displayName": "Dev Proxy",
  "description": "Dev Proxy is a command line tool that helps you simulate behaviors and errors of cloud APIs.",
  "version": "0.0.4",
  "publisher": "garrytrinder",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Snippets",
    "Testing"
  ],
  "activationEvents": [
    "onLanguage:json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/garrytrinder/dev-proxy-vsc-ext.git"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "json",
        "path": "./dist/snippets.json"
      }
    ]
  },
  "preview": true,
  "pricing": "Free",
  "icon": "dist/icon.png",
  "homepage": "https://github.com/garrytrinder/dev-proxy-vsc-ext/blob/main/README.md",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "gts lint",
    "test": "vscode-test",
    "clean": "gts clean",
    "fix": "gts fix"
  },
  "devDependencies": {
    "@types/json-to-ast": "^2.1.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.8.2",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "copy-webpack-plugin": "^12.0.1",
    "eslint": "^8.56.0",
    "ts-loader": "^9.5.1",
    "typescript": "~5.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "gts": "^5.2.0"
  },
  "dependencies": {
    "json-to-ast": "^2.1.0"
  }
}